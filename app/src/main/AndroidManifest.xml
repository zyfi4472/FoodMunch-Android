<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodmunch">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="Food Munch"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Foodmunch">
        <activity
            android:name=".recieveorder"
            android:exported="true" />
        <activity
            android:name=".ordermanagement"
            android:exported="true" />
        <activity
            android:name=".manageorder"
            android:exported="true" />
        <activity
            android:name=".Feedback"
            android:exported="true" />
        <activity
            android:name=".addmenu"
            android:exported="true" />
        <activity
            android:name=".dashboard"
            android:exported="true" />
        <activity
            android:name=".verifyPhone"
            android:exported="false" />
        <activity
            android:name=".editProfile"
            android:exported="false" />
        <activity
            android:name=".userProfile"
            android:exported="false" />
        <activity
            android:name=".specialcustomList"
            android:exported="false" />
        <activity
            android:name=".specialMenu"
            android:exported="false" />
        <activity
            android:name=".home"
            android:theme="@style/Theme.Foodmunch.NoActionBar" />
        <activity
            android:name=".register"
            android:noHistory="true"/>
        <activity
            android:name=".login"
            android:noHistory="true"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>