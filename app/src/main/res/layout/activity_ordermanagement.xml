<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ordermanagement" android:background="#ECC245">

    <Button
            android:id="@+id/button11"
            android:layout_width="226dp"
            android:layout_height="70dp"
            android:text="Received"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/progressBar"
            tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="476dp"
            app:layout_constraintHorizontal_bias="0.497" android:textSize="28sp"
            android:onClick="ordermanagement"/>

    <Button
            android:id="@+id/button13"
            android:layout_width="226dp"
            android:layout_height="70dp"
            android:text="Delivered"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/progressBar"
            tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="264dp"
            app:layout_constraintHorizontal_bias="0.497" android:textSize="28sp"/>
    <Button
            android:id="@+id/button14"
            android:layout_width="226dp"
            android:layout_height="70dp"
            android:text="Cancelled"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="372dp"
            app:layout_constraintHorizontal_bias="0.497" android:textSize="28sp"/>
    <TextView
            android:id="@+id/textView8"
            android:layout_width="247dp"
            android:layout_height="49dp"
            android:fontFamily="sans-serif"
            android:text="Food Munch"
            android:textAlignment="center"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.057" android:gravity="center_horizontal"
            tools:ignore="HardcodedText" android:textColor="#020202" app:layout_constraintHorizontal_bias="0.432"/>
</androidx.constraintlayout.widget.ConstraintLayout>