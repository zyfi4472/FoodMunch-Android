<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dashboard"
    android:background="#ECC245"
    >

    <Button
        android:id="@+id/button6"
        android:layout_width="196dp"
        android:layout_height="57dp"
        android:text="Feedback1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar"
        tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="216dp"
        app:layout_constraintHorizontal_bias="0.451" android:textSize="24sp"
        android:onClick="feedback1"/>
    <Button
        android:id="@+id/button5"
        android:layout_width="196dp"
        android:layout_height="57dp"
        android:text="Menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar"
        tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="512dp"
        app:layout_constraintHorizontal_bias="0.451" android:textSize="24sp"
        android:onClick="menu"/>
    <Button
        android:id="@+id/button4"
        android:layout_height="59dp"
        android:text="Orders"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar"
        tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="412dp"
        app:layout_constraintHorizontal_bias="0.451" android:textSize="24sp"
        android:onClick="orders" android:layout_width="190dp"/>

    <Button
            android:id="@+id/button3"
            android:layout_width="196dp"
            android:layout_height="57dp"
            android:text="Customer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="HardcodedText" android:textColor="#070707" android:layout_marginBottom="312dp"
            app:layout_constraintHorizontal_bias="0.451" android:textSize="24sp"/>
    <TextView
            android:id="@+id/textView3"
            android:layout_width="247dp"
            android:layout_height="49dp"
            android:fontFamily="sans-serif"
            android:text="Food Munch"
            android:textAlignment="center"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.057" android:gravity="center_horizontal"
            tools:ignore="HardcodedText" android:textColor="#020202" app:layout_constraintHorizontal_bias="0.432"/>
</androidx.constraintlayout.widget.ConstraintLayout>
